{
	"name": "dmzx/quotescollection",
	"type": "phpbb-extension",
	"description": "The DM Quotes Collection is a simple tool, where you can start collecting quotes. These will be display on the index page in a random order. There are different settings possible in the ACP, where you also can manage the quotes.",
	"homepage": "http://www.dmzx-web.net",
	"version": "1.0.3",
	"time": "2015-10-25",
	"keywords": ["phpbb", "extension", "quotescollection"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "http://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Quotes Collection",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0,<3.2.0@dev"
		},
		"version-check": {
			"host": "www.dmzx-web.net",
			"directory": "/versions",
			"filename": "quotescollection_version.json"
		}
	}
}
